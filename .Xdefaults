Xft*dpi:                    96

xautolock*detectsleep:      true
xautolock*time:             30

xlock*mode:                 image
xlock*bitmap:               .xlock.xpm
xlock*erasedelay:           0
xlock*count:                1
xlock*background:           black
xlock*foreground:           white

urxvt*urgentOnBell:         true
urxvt*secondaryScroll:      true
urxvt*buffered:             true
urxvt*background:           black
urxvt*foreground:           white
urxvt*color12:              rgb:5c/5c/ff
urxvt*scrollBar:            false
urxvt*skipBuiltinGlyphs:    true
urxvt*font:                 xft:Dejavu Sans Mono:book:size=10
urxvt*fontBold:             xft:Dejavu Sans Mono:bold:size=10
urxvt*perl-lib:             /usr/lib/urxvt/perl/
urxvt*perl-ext-common:      default,confirm-paste,matcher
urxvt*urlLauncher:          xdg-open
urxvt*matcher.button:       3
urxvt*searchable-scrollback: CM-s
urxvt*saveLines:            65535
urxvt*scrollTtyOutput:      false
urxvt*scrollTtyKeypress:    true
urxvt*scrollWithBuffer:     true
